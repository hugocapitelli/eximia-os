{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Partnerships Output",
    "type": "object",
    "properties": {
        "partnership_proposal": {
            "type": "object",
            "properties": {
                "target_partner": {
                    "type": "string"
                },
                "model": {
                    "type": "string"
                },
                "offer": {
                    "type": "string"
                },
                "expected_volume": {
                    "type": "string"
                },
                "risk_assessment": {
                    "type": "string"
                }
            },
            "required": [
                "target_partner",
                "offer"
            ]
        }
    },
    "required": [
        "partnership_proposal"
    ]
}