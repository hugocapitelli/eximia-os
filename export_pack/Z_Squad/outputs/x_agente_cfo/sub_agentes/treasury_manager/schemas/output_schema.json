{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Treasury Manager Output",
    "type": "object",
    "properties": {
        "cash_position_report": {
            "type": "object",
            "properties": {
                "total_liquidity": {
                    "type": "string"
                },
                "runway_months": {
                    "type": "string"
                },
                "alert_level": {
                    "type": "string",
                    "enum": [
                        "GREEN",
                        "YELLOW",
                        "RED"
                    ]
                }
            },
            "required": [
                "runway_months",
                "alert_level"
            ]
        }
    },
    "required": [
        "cash_position_report"
    ]
}